    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "Celery",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "console": "integratedTerminal",
            "args": [
                "-A",
                "celery_app:app",
                "worker",
                "-l",
                "info",
                "-n",
                "tnr_imgcmprs",
                "-Q",
                "tnr_imgcmprs_queue"
            ]
        }
    ]
}